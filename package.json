{
  "name": "tieba",
  "publisher": "wister",
  "displayName": "tieba",
  "description": "用 vscode 看贴吧",
  "repository": {
    "type": "git",
    "url": "https://github.com/Iwctwbh/vscode-tieba"
  },
  "icon": "img/paste.png",
  "keywords": [
    "tieba"
  ],
  "version": "0.1.2",
  "engines": {
    "vscode": "^1.50.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:tieba-index"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tieba-index.refresh",
        "title": "刷新",
        "icon": "img/refresh.svg"
      },
      {
        "command": "tieba.delete",
        "title": "删除"
      },
      {
        "command": "tieba.search",
        "title": "搜索"
      },
      {
        "command": "tieba.openExternal",
        "title": "用浏览器打开",
        "icon": "img/link.svg"
      },
      {
        "command": "tieba.add",
        "title": "添加贴吧",
        "icon": "img/link.svg"
      },
      {
        "command": "tieba.cookie",
        "title": "设置cookie",
        "icon": "img/link.svg"
      },
      {
        "command": "tieba.fontColor",
        "title": "设置字体颜色",
        "icon": "img/link.svg"
      }
    ],
    "keybindings": [],
    "menus": {
      "editor/context": [],
      "view/title": [
        {
          "command": "tieba.add",
          "when": "view == tieba-index",
          "group": "inline"
        },
        {
          "command": "tieba.cookie",
          "when": "view == tieba-index",
          "group": "inline"
        },
        {
          "command": "tieba.fontColor",
          "when": "view == tieba-index",
          "group": "inline"
        }
      ],
      "view/item/context": [
        {
          "command": "tieba.search",
          "when": "view == tieba-index && viewItem == 1",
          "group": "inline@1"
        },
        {
          "command": "tieba.delete",
          "when": "view == tieba-index && viewItem == 1",
          "group": "inline@2"
        },
        {
          "command": "tieba-index.refresh",
          "when": "view == tieba-index && viewItem == 1",
          "group": "inline@3"
        },
        {
          "command": "tieba.openExternal",
          "when": "view == tieba-index && viewItem == 0",
          "group": "inline"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "tieba",
          "title": "tieba",
          "icon": "img/paste.svg"
        }
      ]
    },
    "views": {
      "tieba": [
        {
          "id": "tieba-index",
          "name": "首页"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js --disable-extensions",
    "jest": "jest",
    "mocha": "mocha",
    "build": "vsce package",
    "test-api": "ts-node ./src/api/search"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.10.3",
    "@types/vscode": "^1.84.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "glob": "^10.3.10",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "vsce": "^2.15.0",
    "vscode-test": "^1.6.1"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "iconv-lite": "^0.6.3",
    "vue": "^3.3.9"
  }
}
